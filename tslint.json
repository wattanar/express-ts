{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended"],
	"jsRules": {},
	"rules": {
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore",
			"allow-pascal-case"
		],
		"indent": [false, "spaces", 2],
		"ordered-imports": false,
		"no-console": false
	},
	"rulesDirectory": []
}
