{
  "name": "express-ts",
  "version": "1.0.0",
  "description": "expressjs with typescript",
  "keywords": [
    "express",
    "typescript"
  ],
  "author": "Wattana Ruengmucha",
  "license": "MIT",
  "scripts": {
    "build": "yarn lint && ./node_modules/.bin/tsc",
    "dev": "./node_modules/.bin/nodemon",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "start:win": "set NODE_ENV=production && node dist/server.js",
    "start:linux": "export NODE_ENV=production && node dist/server.js"
  },
  "dependencies": {
    "@types/compression": "^0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/helmet": "^0.0.42",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "typescript": "^3.4.5",
    "xss": "^1.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0"
  }
}
